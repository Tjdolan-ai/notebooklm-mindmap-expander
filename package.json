{
  "name": "notebooklm-mindmap-expander",
  "version": "1.0.0",
  "description": "Chrome extension to auto-expand NotebookLM mind maps",
  "main": "dist/background.js",
  "scripts": {
    "dev": "npm run clean && npm run build -- --watch",
    "build": "npm run clean && npm run build:ts",
    "build:ts": "esbuild src/*.ts --bundle --outdir=dist --format=iife --platform=browser --target=es2020",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "keywords": [
    "chrome-extension",
    "notebooklm",
    "mind-map",
    "productivity"
  ],
  "author": "Visionary42",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.8",
    "typescript": "^5.3.2",
    "vitest": "^1.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Tjdolan-ai/notebooklm-mindmap-expander.git"
  },
  "bugs": {
    "url": "https://github.com/Tjdolan-ai/notebooklm-mindmap-expander/issues"
  },
  "homepage": "https://github.com/Tjdolan-ai/notebooklm-mindmap-expander#readme"
}